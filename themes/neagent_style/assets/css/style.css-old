html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td {
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-size: 100%;
	 
	 
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
:focus {
	outline: 0;
}
ins {
	text-decoration: none;
}
del {
	text-decoration: line-through;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
html {
	height: 100%;
}
body {
	font: 12px/18px Arial, Tahoma, Verdana, sans-serif;
	height: 100%;
	background-color:#fff;
}
a {
	color: blue;
	outline: none;
	text-decoration: underline;
}
a:hover {
	text-decoration: none;
}
p {
	margin: 0 0 18px
}
img {
	border: none;
}
input {
	vertical-align: middle;
}
#wrapper {
	width: 100%;
	min-width: 1000px;
	min-height: 100%;
	height: auto !important;
	height: 100%;  position: relative;
}


/* Header
-----------------------------------------------------------------------------*/
#header {
	height: 164px;
	border-bottom:1px solid #ccc; position: relative; 
	
}
#headerins {padding-left:40px; padding-right:40px;padding-top:14px;
}


#topnav-logo { 

 
background-image: url("../images/neagent_logo.png");
 
display: block;
float: left;
height: 150px;
text-indent: -9999px;
width: 150px;
}






/* Middle
-----------------------------------------------------------------------------*/
#middle {
	width: 100%;
	padding: 0 0 150px;
	height: 1%;
}
#middle:after {
	content: '.';
	display: block;
	clear: both;
	visibility: hidden;
	height: 0;
}
#container {
	width: 100%;
	float: left;
	ohverflow: hidden;  
}
#container #content {
clear:both;  
  
	padding: 0 324px 0 224px;
}


/* Sidebar Left
-----------------------------------------------------------------------------*/
#sideLeft {
	float: left;
	width: 204px;
	margin-left: -100%;
	position: relative;
	 
	
}


#sideLeftins{
border-right:1px solid #ccc;
padding-left:40px;
padding-top:14px;
}



/* Sidebar Right
-----------------------------------------------------------------------------*/
#sideRight {
	float: left;
	margin-right: -3px;
	width: 304px;
	margin-left: -304px;
	position: relative;
	 
}
#sideRightins{
border-left:1px solid #ccc;
padding-right:40px;
padding-top:14px;padding-left:14px;
}

/* Footer
-----------------------------------------------------------------------------*/
#footer {  poesition: relative; 
	margin: -150px auto 0;
	min-width: 1000px;
	height: 150px;
	border-top:1px solid #ccc;
	
		
	border:0px;
	border-top:2px solid #ccc;
 
left: 0;
bottom: 0;
 width: 100%;
	
  position: absolute;
  bottom: 0;
  
		
}


div#bottomtext{height:150px; background-color:#fff;}
div#bottomtext table {height:150px;  width: 900px;}

 
.f_container{z-index:30; position:relative;height:150px; width:100%;border-top:2px solid #3480b7; 
background:#62a5d5 url('assets/images/f_bg.gif') top left no-repeat; font-size:12px; padding-top:20px;}

.f_thanks{margin-left:216px; background-image:url(<ion:theme_url />assets/images/thanks.png); text-indent:-3000px; width:132px; height:83px;  }
.footer_info{ width:618px; float:right; }
.f_counter{padding-top:5px;width:119px;float:right;height:100px; font-size:11px; color:#0f5e96; line-height:14px;}
.f_counter a:link, .f_counter a:visited {color:#0f5e96;} 
.f_block{}
.f_menu a:link,.f_menu a:visited, .f_menu a:hover, .f_menu{color:white; margin-bottom:5px;}
.f_menu a:hover {background-color:#3581b8; }
.f_links a:link,.f_links a:active,  .f_links a:visited,  .f_links {color:#3380b7;margin-bottom:5px;}
.f_links a:hover {color:#0f5e96; }
.f_email a:link, .f_email{color:#0f5e96;}






